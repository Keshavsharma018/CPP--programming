#include <iostream>
using namespace std;
int main()
{
int SP,CP;
std::cout<<"Enter the Cost Price =";
std::cin>>CP;
std::cout<<"Enter the Selling Price =";
std::cin>>SP;
if(SP>CP){
    std::cout<<"Profit"<<endl<<"The profit made is ="<<SP-CP;
}
else if(SP=CP){
    std::cout<<"Neither Profit Nor Loss"<<endl;
}
else{
    std::cout<<"Loss"<<endl<<"The Loss is ="<<CP-SP;
}
    return 0;
}
