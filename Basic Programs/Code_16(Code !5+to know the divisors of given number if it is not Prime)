#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter the number ";
    cin>>n;
    
            int key;
    for(int i=2;i<=n/2;i++){
        if(n%i==0){
            key=0;
            cout<< i<<endl;
        }
        else{
            key=1;
        }
    }
    if(key==0){
        cout<<"the number is not prime";
    }
    else{
        cout<<"the number is prime";
        
    }
    return 0;
}
