#include<iostream>
using namespace std;
int main(){
    char ch;
    cout<<"enter any character : ";
    cin>>ch;
    if(ch>='A' and ch<='Z'){
        cout<<"Upper case letter";
    }
    else if(ch>='a' and ch<='z'){
        cout<<"Lower case letter";
    }
    else{
        cout<<"Enteterd term is not a character";
    }
    return 0;
}
    
