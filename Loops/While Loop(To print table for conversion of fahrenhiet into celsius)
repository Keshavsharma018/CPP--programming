
#include <iostream>
using namespace std;
int main() {
   float initial_value , final_value , step_up_value , cel , fah ;
   cout<<"Enter the initial value : ";
   cin>>initial_value;
   cout<<"Enter the final value : ";
   cin>>final_value;
   cout<<"Enter the step up value : ";
   cin>>step_up_value;
   cout<<"Fahrenheit"<<    "-"         <<"celsius\n";
   fah=initial_value;
   while(fah<=final_value){
       
       cel=5.0/9.0*(fah-32);
       
       cout<< fah << "    \t    " << cel <<endl;
       
       fah=fah+step_up_value;
   }
    return 0;
}
