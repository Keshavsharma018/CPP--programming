#include<stdio.h>
#include<bits/stdc++.h>
using namespace std;
int main(){
    int arr[]={3,-5,-6,7,8};
    int n= sizeof(arr)/sizeof(int);
     int sum=0;
     int ans=0;
     for(int i=0;i<n;i++){
         sum+=arr[i];
         if (sum<0){
             sum=0;
         }
             ans=max(ans,sum);
     }
    cout<<ans;
    return 0;
}
